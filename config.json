{
    "hyper_params": {
        "batch_size": 128,
        "num_epochs": 100,
        "lr": 1e-4,
        "weight_decay": 0.0001,
        "momentum": 0.9
    },
    "model_params": [
        {
            "type": "Conv",
            "in_channels": 3,
            "out_channels": 4,
            "kernel_size": [7],
            "in_size": 48,
            "out_size": 24
        },
        {
            "type": "Conv",
            "in_channels": 4,
            "out_channels": 8,
            "kernel_size": [7],
            "in_size": 24,
            "out_size": 12
        },
        {
            "type": "Conv",
            "in_channels": 8,
            "out_channels": 16,
            "kernel_size": [5],
            "in_size": 12,
            "out_size": 6
        },
        {
            "type": "Conv",
            "in_channels": 16,
            "out_channels": 32,
            "kernel_size": [5],
            "in_size": 6,
            "out_size": 3
        },
        {
            "type": "Conv",
            "in_channels": 32,
            "out_channels": 64,
            "kernel_size": [3],
            "in_size": 3,
            "out_size": 1
        }
    ]
}
